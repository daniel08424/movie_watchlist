<h1>Movie Details</h1>
<% if (movieData && movieData.length > 0) { %>
    <% movieData.forEach(function(movie) { %>
        <div>
            <h2><%= movie.Title %></h2>
            <p>Year: <%= movie.Year %></p>
            <p>Type: <%= movie.Type %></p>
            <img src="<%= movie.Poster %>" alt="Movie Poster">
        </div>
    <% }); %>
<% } else { %>
    <p>No movie details available.</p>
<% } %>